{
  "name": "react-markdown-preview-editor",
  "version": "1.2.5",
  "description": "A markdown editor based on marked",
  "main": "lib/index.js",
  "scripts": {
    "start": "set NODE_ENV=dev && webpack-dev-server --open",
    "build": "rimraf lib && babel src --out-dir lib && webpack --config webpack.config.prod.js",
    "test": "jest --coverage",
    "demo": "webpack --config webpack.config.example.js",
    "lint": "eslint src/ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZiQiangWang/react-markdown-editor.git"
  },
  "keywords": [
    "markdown",
    "editor",
    "react",
    "marked",
    "table"
  ],
  "author": "wangziqiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZiQiangWang/react-markdown-editor/issues"
  },
  "homepage": "https://github.com/ZiQiangWang/react-markdown-editor#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "codemirror": "^5.27.4",
    "react-dom": "^15.6.1"
  }
